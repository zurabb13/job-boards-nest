{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/graphql",
      "dest": "src/main.ts",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "PATCH", "DELETE"]
    }
  ]
}
